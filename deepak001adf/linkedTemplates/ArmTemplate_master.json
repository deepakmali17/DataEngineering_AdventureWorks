{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "deepak001adf"
		},
		"AzureDataLakeStorage_LS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage_LS'"
		},
		"AzureDatabricks_DS_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks_DS'"
		},
		"AzureDataLakeStorage_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://deepakadls001.dfs.core.windows.net/"
		},
		"AzureDatabricks_DS_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0212-160259-i0vpfste"
		},
		"HttpServer_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/deepakmali17/DataEngineering_AdventureWorks/refs/heads/main/data/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "deepak001adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage_LS_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_LS_accountKey')]"
					},
					"AzureDatabricks_DS_accessToken": {
						"value": "[parameters('AzureDatabricks_DS_accessToken')]"
					},
					"AzureDataLakeStorage_LS_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_LS_properties_typeProperties_url')]"
					},
					"AzureDatabricks_DS_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks_DS_properties_typeProperties_existingClusterId')]"
					},
					"HttpServer_LS_properties_typeProperties_url": {
						"value": "[parameters('HttpServer_LS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}